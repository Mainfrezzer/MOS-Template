{
  "name": "Roundcubemail",
  "repo": "roundcube/roundcubemail:latest",
  "registry": "https://hub.docker.com/r/roundcube/roundcubemail/",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "sh",
  "privileged": false,
  "extra_parameters": null,
  "post_parameters": null,
  "web_ui_url": "http://[IP]:[PORT:80]",
  "icon": "https://upload.wikimedia.org/wikipedia/commons/thumb/e/e3/Roundcube_logo_icon.svg/240px-Roundcube_logo_icon.svg.png",
  "project": "https://github.com/roundcube/roundcubemail-docker",
  "support": "https://forums.unraid.net/topic/182778-support-mainfrezzer-docker-repo/",
  "description": "Roundcube Mail is an open-source webmail software that offers a user-friendly interface for managing email accounts. \r\n  It features a rich text editor, address book, and supports multiple email protocols, making it a popular choice for individuals and organizations seeking a customizable web-based email solution.\r\n  ",
  "paths": [
    {
      "name": "www-data",
      "host": "/mnt/Magnon/appdata/roundcubemail/html",
      "container": "/var/www/html",
      "mode": "rw",
      "description": "Path for the webinterface",
      "required": true
    },
    {
      "name": "Database",
      "host": "/mnt/Magnon/appdata/roundcubemail/db",
      "container": "/var/roundcube/db",
      "mode": "rw",
      "description": "Path for the databank",
      "required": true
    }
  ],
  "ports": [
    {
      "name": "Webgui",
      "host": "9002",
      "container": "80",
      "protocol": "tcp",
      "description": "Port for the webserver",
      "required": true,
      "mask": false
    }
  ],
  "variables": [
    {
      "name": "Default Host",
      "key": "ROUNDCUBEMAIL_DEFAULT_HOST",
      "value": "ssl://mail.example.com",
      "description": " For encrypted connections, prefix the host with tls:// (STARTTLS) or ssl:// (SSL/TLS).",
      "required": true,
      "mask": false
    },
    {
      "name": "Default Port",
      "key": "ROUNDCUBEMAIL_DEFAULT_PORT",
      "value": "993",
      "description": "IMAP port number; defaults to 143 (993 for ssl)",
      "required": true,
      "mask": false
    },
    {
      "name": "SMTP Server",
      "key": "ROUNDCUBEMAIL_SMTP_SERVER",
      "value": "ssl://mail.example.com",
      "description": "For encrypted connections, prefix the host with tls:// (STARTTLS) or ssl:// (SSL/TLS).",
      "required": true,
      "mask": false
    },
    {
      "name": "SMTP Port",
      "key": "ROUNDCUBEMAIL_SMTP_PORT",
      "value": "465",
      "description": "SMTP port number; defaults to 587 (465 for ssl)",
      "required": true,
      "mask": false
    },
    {
      "name": "DB-Type",
      "key": "ROUNDCUBEMAIL_DB_TYPE",
      "value": "sqlite",
      "description": null,
      "required": true,
      "mask": false
    },
    {
      "name": "Username",
      "key": "ROUNDCUBEMAIL_USERNAME_DOMAIN",
      "value": "example.com",
      "description": "Automatically add this domain to user names for login.",
      "required": false,
      "mask": false
    }
  ],
  "devices": [],
  "labels": []
}
