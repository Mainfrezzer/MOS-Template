{
  "name": "bashupload",
  "repo": "ghcr.io/mainfrezzer/bashupload:latest",
  "registry": "https://github.com/Mainfrezzer/docker-bashupload/pkgs/container/bashupload",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "sh",
  "privileged": false,
  "extra_parameters": "-m 1G",
  "post_parameters": null,
  "web_ui_url": "http://[IP]:[PORT:80]",
  "icon": "https://raw.githubusercontent.com/Mainfrezzer/UnRaid-Templates/main/icon/bashup.png",
  "project": "https://github.com/IO-Technologies/bashupload",
  "support": "https://forums.unraid.net/topic/182778-support-mainfrezzer-docker-repo/",
  "description": "A containerized selfhost version of https://bashupload.com/\r\n\r\nPHP/JavaScript file upload web app to upload files from command line &amp; browser, and download them elsewhere. Frequently used to upload/download files on servers.\r\n\r\n    Huge thanks to https://github.com/siomochkin for the icon\n  ",
  "paths": [
    {
      "name": "App Location",
      "host": "/mnt/Magnon/appdata/bashupload",
      "container": "/app",
      "mode": "rw",
      "description": "Location for the installed app alongside the configuration files",
      "required": true
    },
    {
      "name": "Filestorage",
      "host": "/mnt/Magnon/bashupload",
      "container": "/app/files",
      "mode": "rw",
      "description": "Storage location for all uploaded files",
      "required": true
    }
  ],
  "ports": [
    {
      "name": "Webserver",
      "host": "5465",
      "container": "80",
      "protocol": "tcp",
      "description": null,
      "required": true,
      "mask": false
    }
  ],
  "variables": [
    {
      "name": "Upload size",
      "key": "UPLOADSIZE",
      "value": "512M",
      "description": "Use 128M, 512M, 1G or 10G to set the size.",
      "required": true,
      "mask": false
    },
    {
      "name": "UID",
      "key": "UID",
      "value": "500",
      "description": null,
      "required": true,
      "mask": false
    },
    {
      "name": "GID",
      "key": "GID",
      "value": "500",
      "description": null,
      "required": true,
      "mask": false
    }
  ],
  "devices": [],
  "labels": []
}
