{
  "name": "WGDashboard",
  "repo": "donaldzou/wgdashboard:latest",
  "registry": "https://github.com/donaldzou/WGDashboard/pkgs/container/wgdashboard",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "sh",
  "privileged": false,
  "extra_parameters": "--cap-add=NET_ADMIN --sysctl net.ipv4.conf.all.src_valid_mark=1",
  "post_parameters": null,
  "web_ui_url": "http://[IP]:[PORT:10086]",
  "icon": "https://raw.githubusercontent.com/Mainfrezzer/UnRaid-Templates/refs/heads/main/icon/WGDashboard.png",
  "project": "https://github.com/donaldzou/WGDashboard",
  "support": "https://forums.unraid.net/topic/182778-support-mainfrezzer-docker-repo/",
  "description": "WGDashboard is a docker container that lets you deploy amnezia and wireguard server over a webinterface. \r\n\r\nhttps://github.com/donaldzou/WGDashboard\r\n\r\n\r\nIf you have ipv6, dont forget to add \"--sysctl net.ipv6.conf.all.forwarding=1\" to the extra parameters",
  "paths": [
    {
      "name": "Data",
      "host": "/mnt/Magnon/appdata/WGDashboard/data",
      "container": "/data",
      "mode": "rw",
      "description": null,
      "required": true
    },
    {
      "name": "Amnezia-Server-Config",
      "host": "/mnt/Magnon/appdata/WGDashboard/amneziawg",
      "container": "/etc/amnezia/amneziawg",
      "mode": "rw",
      "description": null,
      "required": true
    },
    {
      "name": "Wireguard-Server-Config",
      "host": "/mnt/Magnon/appdata/WGDashboard/wireguard",
      "container": "/etc/wireguard",
      "mode": "rw",
      "description": null,
      "required": true
    }
  ],
  "ports": [
    {
      "name": "WebUI",
      "host": "10086",
      "container": "10086",
      "protocol": "tcp",
      "description": null,
      "required": true,
      "mask": false
    },
    {
      "name": "Wireguard/Amnezia Port",
      "host": "51820",
      "container": "51820",
      "protocol": "udp",
      "description": null,
      "required": true,
      "mask": false
    }
  ],
  "variables": [
    {
      "name": "DNS Servers",
      "key": "global_dns",
      "value": "9.9.9.9, 2620:fe::fe, 8.8.8.8, 2001:4860:4860::8888, 1.1.1.1, 2606:4700:4700::1111",
      "description": null,
      "required": true,
      "mask": false
    }
  ],
  "devices": [],
  "labels": []
}
